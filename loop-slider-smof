

<!-- SMOF Data will start -->
	<?php global $data;
    $slides = $data['loop_slider']; // this id have to into SMOF 
    foreach ($slides as $slide) { ?>
	
		<!-- our data start -->
		   <a href="<?php echo $slide['link']; ?>">
				<img src="<?php echo $slide['url']; ?>" alt="" title="<?php echo $slide['description']; ?>" />
		   </a>
		<!-- our data end>
		
    <?php }
    ?>  
<!-- SMOF Data will end -->
